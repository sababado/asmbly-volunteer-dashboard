{
    "name": "asmbly-volunteer-dashboard-root",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "dev": "concurrently \"npm run dev -w @asmbly/frontend\" \"npm run storybook -w @asmbly/ui-kit\"",
        "build": "npm run build --workspaces --if-present",
        "lint": "npm run lint --workspaces --if-present",
        "test": "npm run test --workspaces --if-present"
    },
    "devDependencies": {
        "concurrently": "^9.2.1"
    },
    "overrides": {
        "@types/react": "^18.2.0",
        "@types/react-dom": "^18.2.0"
    }
}